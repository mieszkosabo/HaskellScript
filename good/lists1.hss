const list = [1, 2, 3];
print(list);

const emptyList = [];
print(emptyList);

match list:
  case [] {
    print("empty!");
  }
  case x:xs {
    print("head: ", x);
    print("tail: ", xs);
  }

const isMember = \a, l => {
  match l:
    case [] {
      return false;
    }
    case x:xs {
      return x == a ? true : isMember(a, xs);
    }
};

print(isMember(4, list)); // false
print(isMember(2, list)); // true